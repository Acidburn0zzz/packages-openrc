# Maintainer: udeved <udeved@openrc4arch.site40.net>

_rn="privoxy"
_ver=20130325

pkgname=${_rn}-openrc
pkgver=1.1.1
pkgrel=1
pkgdesc="OpenRC ${_rn} init script"
arch=('any')
url="http://openrc4arch.site40.net"
license=('GPL2')
depends=('openrc' "${_rn}")
provides=('openrc-proxy')
install=${_rn}.install
source=("http://openrc4arch.site40.net/pool/archlinux/${_rn}-${_ver}.tar.xz")

package() {
	install -Dm755 "$srcdir/${_rn}/init.d/privoxy.initd" "$pkgdir/etc/init.d/privoxy"
}
sha256sums=('cf55f0832496e7f85fc429bb0e038c74e77a07e66760c1e8c525391b4f5e097e')
