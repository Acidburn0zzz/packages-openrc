# Maintainer: udeved <udeved@openrc4arch.site40.net>

_pn=openrc-plugins
_rn=catalyst-utils

pkgname=$_rn-openrc
pkgver=0.9.3
pkgrel=1
pkgdesc="OpenRC $_rn atieventsd init script"
arch=('any')
url="http://openrc4arch.site40.net"
license=('GPL2')
depends=('openrc' "$_rn" 'udev-init-scripts')
install=$_rn.install
source=("$url/pool/archlinux/$_pn-$pkgver.tar.xz")

package() {
	install -Dm755 "$srcdir/$_pn/$_rn/init.d/atieventsd.init" "$pkgdir/etc/init.d/atieventsd"
}
sha256sums=('7f798793e431ab873f9677dc30560251d23aeb812da941f8c034ba6bddc3c0c7')
            
