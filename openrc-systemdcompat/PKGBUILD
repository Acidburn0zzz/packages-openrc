pkgname=openrc-systemdcompat
pkgver=0.2
pkgrel=1
pkgdesc="openrc wrapper(s) for systemd components"
arch=('any')
license=('GPL')
groups=('openrc' 'openrc-base')
url="https://github.com/aadityabagga/scripts"
depends=('openrc-core')
#conflicts=('systemd')
source=("https://raw.githubusercontent.com/aadityabagga/scripts/bb30b53d0d4cfb05febebb794c3fce944a64e1d9/systemctl")
sha1sums=('8af8d82d159ff7f6a94be574ae9ff5bea067a34d')

package() {
  install -Dm 755 systemctl $pkgdir/usr/bin/systemctl
}
