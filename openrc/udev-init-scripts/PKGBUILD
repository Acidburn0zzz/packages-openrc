# Maintainer: udeved <udeved@openrc4arch.site40.net>

pkgname=udev-init-scripts
pkgver=25
pkgrel=2
pkgdesc="OpenRC udev init scripts"
arch=('any')
url="http://openrc4arch.site40.net"
license=('GPL2')
groups=('openrc-base')
depends=('udev' 'openrc')
install=udev.install
source=("http://dev.gentoo.org/~williamh/dist/$pkgname-$pkgver.tar.bz2")
sha256sums=('2210a0c76b1169d136b3f7c8f8c6ca36b4546aea7e88c355c81009cf10fb9bc3')

package() {
	
	cd "${srcdir}/${pkgname}-${pkgver}"
	make DESTDIR="${pkgdir}" install
	
}
