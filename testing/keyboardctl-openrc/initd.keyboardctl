#!/usr/bin/runscript

depend() {
	need dbus
}

start() {
	ebegin "Starting ${SVCNAME}"
	start-stop-daemon --start --background --quiet --exec /usr/bin/keyboardctl -- "${OPTS}"
	eend $?
}

stop() {
	ebegin "Stopping ${SVCNAME}"
	start-stop-daemon --stop --background --quiet --exec /usr/bin/keyboardctl -- "${OPTS}"
	eend $?
}
