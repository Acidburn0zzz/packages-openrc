#!/usr/bin/runscript

depend() {
	need dbus
	use consolekit xdm
}

start() {
	ebegin "Starting pamac-daemon"
	start-stop-daemon --background  --quiet --exec /usr/bin/pamac-daemon
	eend $?
}

stop() {
	ebegin "Stopping pamac-daemon"
	start-stop-daemon --background  --quiet --exec /usr/bin/pamac-daemon
	eend $?
}
